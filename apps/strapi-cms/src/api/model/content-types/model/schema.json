{
  "kind": "collectionType",
  "collectionName": "models",
  "info": {
    "singularName": "model",
    "pluralName": "models",
    "displayName": "Model",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "modelKit": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::model-kit.model-kit"
    },
    "buildStatus": {
      "type": "enumeration",
      "enum": [
        "UNBUILT",
        "BUILDING",
        "COMPLETED"
      ]
    },
    "completionDate": {
      "type": "date"
    },
    "projectId": {
      "type": "customField",
      "customField": "plugin::clockify.clockify_projects"
    },
    "modelTags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::model-tag.model-tag"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "youtubeId": {
      "type": "string"
    },
    "buildLog": {
      "type": "component",
      "repeatable": true,
      "component": "models.build"
    },
    "reviewPost": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::post.post"
    }
  }
}
